<table>
  <thead>
    <th>Id</th>
    <th>Company Code</th>
    <th>Company Name</th>
    <th>Address 1</th>
    <th>Address 2</th>
    <th>Email Id </th>
    <th>Contact no</th>
  </thead>
  <tbody>
    <% @mcompany.each do |c| %>
      <tr>
        <td><%= c.id %></td>
        <td><%= c.company_cd %></td>
        <td><%= c.company_name %></td>
        <td><%= c.address1 %></td>
        <td><%= c.address2 %></td>
        <td><%= c.email_id %></td>
        <td><%= c.contact_no %></td>
        <td><%= link_to 'Edit', edit_m_company_path(id: c.id) %></td>
        <td><%= link_to 'Delete', m_company_path(id: c.id) , method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>