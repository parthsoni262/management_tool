<table>
  <thead>
    <th>Id</th>
    <th>Role Name </th>
  </thead>
  <tbody>
    <% @mrole.each do |c| %>
      <tr>
        <td><%= c.id %></td>
        <td><%= c.role_name %></td>
        <td><%= link_to 'Edit', edit_m_role_path(id: c.id) %></td>
        <td><%= link_to 'Delete', m_role_path(id: c.id), method: :delete, data: {confirm: 'Are you sure you want to delete this '} %></td>
      </tr>
    <% end %>
  </tbody>
</table>