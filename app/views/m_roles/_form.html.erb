<h2>M Role </h2>
<div class="container">
  <%= form_for @mrole do |f| %>
    <div class="form-group">
      <%= f.label :role_name %><br>
      <%= f.text_field :role_name, autocomplete: "role_name", class: 'form-control' %>
    </div>
    <br>
    <div class="form-group">
        <%= f.submit "Submit", class: 'btn btn-primary btn-block' %>
    </div>
  <% end %>
</div>