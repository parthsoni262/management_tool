<h2>M User </h2>
<div class="container">
  <%= form_for @muser do |f| %>
    <div class="form-group">
      <%= f.label :user_id %><br>
      <%= f.text_field :user_id, autocomplete: "user_id", class: 'form-control' %>
    </div>
    <br>
    <div class="form-group">
        <%= f.label :mail_id %><br>
        <%= f.text_field :mail_id, autocomplete: "mail_id", class: 'form-control'  %>
    </div>
    <div>
      <%= f.label :m_company_id %>
      <%#= f.select :id, options_for_select(@companies.all.map{ |s| [s.company_name, s.id] })%>
      <%#= @companies %>
      <%= f.select :m_company_id , @companies %>
      <%#= f.collection_select :m_company_id, @companies, :id, :company_name %>
    </div>
    <br>
    <div class="form-group">
        <%= f.label :password %><br>
        <%= f.password_field :password, autocomplete: "password", class: 'form-control'  %>
    </div>
    <br>
    <div class="form-group">
        <%= f.submit "Submit", class: 'btn btn-primary btn-block' %>
    </div><br>
  <% end %>
</div>